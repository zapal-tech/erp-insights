{
	"root": true,
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:vue/vue3-recommended",
		"plugin:prettier/recommended"
	],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"parser": "@typescript-eslint/parser"
	},
	"plugins": [
		"vue",
		"prettier"
	],
	"rules": {
		"vue/no-reserved-component-names": "off",
		"vue/multi-word-component-names": "off",
		"no-unused-vars": "off",
	}
}